<div class="container margin-top">
	<div class="row">
		<div class="col-lg-4">
			<table class="content table table-hover table-striped pull-left">
				<thead>
					<tr>
						<th class="col-lg-10">Anime request</th>
						<th class="col-lg-2">{{model.length}}
					</tr>
				</thead>
				<tbody>
					{{#each request in pagedContent}}
						<tr>
							<td colspan="2">
								{{#linkTo request request}}{{request.title}}{{/linkTo}}
							</td>
						</tr>
					{{/each}}
					<tr>
						<td colspan="2">
							{{view App.PaginatorCollectionView contentBinding="controller.paginator"}}
						</td>
					</tr>
				</tbody>
			</table>
		</div>
		{{outlet}}
	
	</div>
</div>