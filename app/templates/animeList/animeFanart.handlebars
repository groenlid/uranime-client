{{#with view.content}}
<div class="anime-gallery gallery-fanart pull-left">
    <div class="gallery-image">
        <a {{action goToAnime this target="parentView" href=true}}> 
            <img {{bindAttr src="fanartURLSmall"}} />
            <span>
                View Anime
            </span>
        </a>
    </div>
    <p>
        {{title}}
    </p>
</div>
{{/with}}
